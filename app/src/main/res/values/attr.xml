<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RippleView">

        <attr name="radius" format="integer" />
        <attr name="stroke_width" format="integer" />
        <attr name="duration" format="integer" />
        <attr name="repeat_count" format="integer" />

        <attr name="two_ripple_times" format="float" />
        <attr name="three_ripple_times" format="float" />
        <attr name="max_more_radius_times" format="float" />

    </declare-styleable>

    <declare-styleable name="TopBar">
        <attr name="title" format="string"/>
        <attr name="titleTextSize" format="dimension"/>
        <attr name="titltTextColor" format="color"/>
        <attr name="leftTextColor" format="color"/>
        <attr name="leftBackGround" format="reference|color"/>
        <attr name="leftText" format="string"/>
        <attr name="rightTextColor" format="color"/>
        <attr name="rightBackGround" format="reference|color"/>
        <attr name="rightText" format="string"/>
    </declare-styleable>
</resources>